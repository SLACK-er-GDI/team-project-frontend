{{#each uploads as |upload|}}
<!-- go through uploads array and do something on each one -->
        <div class="grid col-sm-6 col-md-3">
           <div class="thumbnail">

               <h4><a href="#" class="hb-buttons thumbnail-images contextDisplay" data-trigger="focus" data-toggle="popover" title="Image Properties" data-html="true" data-content="Uploader: {{upload.ownerEmail}}<br />Date Created: {{upload.createdAt}}<br />Last Updated: {{upload.updatedAt}}<br />Description: {{upload.description}}">{{upload.title}}</a></h4>

               <img src="{{upload.url}}" onerror=this.src="https://i.imgur.com/TZljVDd.jpg">
               <!-- <p>{{upload.tags}}</p> -->
               <!-- <p>Uploader: {{upload.ownerEmail}}</p> -->
                {{#if upload.tags}}
                <p>
                {{#each upload.tags as |tag|}}
                {{tag}}
                {{/each}}
                </p>
                {{/if}}

               <a href="{{upload.url}}" class="hb-buttons download-button" download="testit">Download</a>
               <div class="clearfix"></div>
           </div>
         </div>
{{/each}}
