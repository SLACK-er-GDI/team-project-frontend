{{#each uploads as |upload|}}
<!-- go through uploads array and do something on each one -->
        <div class="col-sm-6 col-md-3">
           <div class="thumbnail">
               <h4>{{upload.title}}</h4>
               <img src="{{upload.url}}" alt="...">
               <div class="col-xs-6" ><button data-id="{{upload.id}}" type="button" class="remove btn btn-warning col-xs-6">Remove!</button></div>
               <div class="col-xs-6"><button data-id="{{upload.id}}" type="button"  class="edit btn btn-danger col-xs-6">Update</button></div>
               <div class="clearfix"></div>
           </div>
         </div>
{{/each}}
